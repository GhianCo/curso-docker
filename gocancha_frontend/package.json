{
  "name": "gocancha",
  "version": "0.0.1",
  "author": "GhianCo Zapata",
  "homepage": "https://gocancha.com/",
  "scripts": {
    "ng": "ng",
    "start": "npm run tailwind && ionic serve --external",
    "build": "npm run tailwind:prod && ionic build --configuration production",
    "android": "npm run tailwind:prod && ionic build --prod; npx cap add android; npx cap sync android; npx cap open android",
    "ios": "npm run tailwind:prod && ionic build --prod; npx cap add ios; npx cap sync ios; npx cap open ios",
    "lab": "npm run tailwind && ionic serve --lab --external",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tailwind": "npm run tw:base && npm run tw:utilities && npm run tw:components",
    "tailwind:prod": "PURGE_TW=true npm run tailwind",
    "tw:base": "ngtw b -c ng-tailwind/ng-tailwind.base.js",
    "tw:utilities": "ngtw b -c ng-tailwind/ng-tailwind.utilities.js",
    "tw:components": "ngtw b -c ng-tailwind/ng-tailwind.components.js"
  },
